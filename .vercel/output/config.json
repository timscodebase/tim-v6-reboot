{
  "version": 3,
  "routes": [
    {
      "src": "/_app/immutable/.+",
      "headers": {
        "cache-control": "public, immutable, max-age=31536000"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/render"
    }
  ],
  "overrides": {
    "index.html": {
      "path": ""
    },
    "about.html": {
      "path": "about"
    },
    "projects.html": {
      "path": "projects"
    },
    "resume.html": {
      "path": "resume"
    },
    "uses.html": {
      "path": "uses"
    }
  }
}